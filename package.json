        "lint": "prettier --check . && eslint .",
        "start": "bun ./build/index.js"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.1",
        "@eslint/js": "^9.32.0",
        "@sveltejs/adapter-auto": "^6.0.1",

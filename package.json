        "lint": "prettier --check . && eslint .",
        "start": "bun ./build/index.js"
    },
    "devDependencies": {
        "@eslint/compat": "^1.2.5",
        "@eslint/js": "^9.18.0",
        "@sveltejs/adapter-auto": "^6.0.0",
